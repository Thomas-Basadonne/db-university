1. 
    SELECT COUNT(*) AS "numero degli studenti", YEAR(`enrolment_date`) AS "anno iscrizione"
    FROM `students`
    GROUP BY YEAR(`enrolment_date`);


2.
    SELECT COUNT(*) AS "N. insegnanti", `office_address` AS "indirizzo"
    FROM `teachers`
    GROUP BY `office_address`


3.
    SELECT AVG(`vote`) AS "media voto", `exam_id` AS "id appello"  
    FROM `exam_student`
    GROUP BY `exam_id`


4. 
    SELECT COUNT(*) AS "N. corsi di laurea", `department_id` AS "id dipartimento" 
    FROM `degrees`
    GROUP BY `department_id`
